{
  "name": "immersive-prep",
  "version": "1.0.0",
  "description": "Repository for preperation immersive course.",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:example": "jest test-example/",
    "test:recursion": "jest recursion-review/",
    "test:underbar": "jest underbar-review/",
    "test:watch": "jest --watch",
    "test:circleci": "jest --json --outputFile=.circleci/results.json",
    
    "lint": "eslint **/*.js --quiet",
    "check": "node .airci/submit.js",
    "submit-test": "node __test__/getReview && mocha server/spec/server-spec.js --reporter reporter.js --exit",
    "submit": "npm-run-all -c -s --silent submit-test check"

  },
  "keywords": [
    "linter",
    "testing",
    "recursion",
    "underbar"
  ],
  "author": "codesatates",
  "dependencies": {},
